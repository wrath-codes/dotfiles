{
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.fontWeight": "500",
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.fontFamily": "MesloLGSDZ Nerd Font Mono",
    "editor.fontWeight": "500",
    "editor.lineHeight": 0,
    "editor.lineNumbers": "relative",
    "editor.fontLigatures": "true",
    "editor.tabSize": 4,
    "editor.fontSize": 13,
    "security.workspace.trust.untrustedFiles": "open",
    "glassit.alpha": 235,
    "editor.inlineSuggest.enabled": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.fixAll.stylelint": "explicit"
    },
    "editor.formatOnPaste": true,
    "autoimport.autoComplete": true,
    "autoimport.doubleQuotes": true,
    "autoimport.spaceBetweenBraces": true,
    "autoimport.useSemiColon": true,
    "editor.suggestSelection": "first",
    "editor.suggest.showIcons": true,
    "workbench.iconTheme": "bearded-icons",
    "autoimport.showNotifications": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc.js]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "vscode.markdown-language-features"
    },
    "[markdown.js]": {
        "editor.defaultFormatter": "vscode.markdown-language-features"
    },
    "[markdown.vue]": {
        "editor.defaultFormatter": "vscode.markdown-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript][javascriptreact][typescript][typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.formatOnType": true,
    },
    "isort.args": [
        "--profile",
        "black"
    ],
    "editor.minimap.enabled": false,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "jupyter.interactiveWindowMode": "perFile",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.completeFunctionParens": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.defaultInterpreterPath": "",
    "jupyter.askForKernelRestart": false,
    "notebook.lineNumbers": "on",
    "workbench.editorAssociations": {
        "*.csv": "default",
        "*.svg": "default",
        "*.db": "default"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.compactFolders": false,
    "explorer.expandSingleFolderWorkspaces": false,
    "eslint.format.enable": true,
    "eslint.options": {
        "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
        ]
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "tailwindCSS.emmetCompletions": true,
    "typescript.preferences.quoteStyle": "double",
    "advancedNewFile.exclude": {
        "node_modules": true,
        "node_modules_electron": true,
        "dev": true,
        "dist": true
    },
    "advancedNewFile.showInformationMessages": true,
    "advancedNewFile.convenienceOptions": [
        "last",
        "current",
        "root"
    ],
    "editor.rename.enablePreview": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "errorLens.statusBarIconsEnabled": true,
    "errorLens.gutterIconsEnabled": true,
    "errorLens.statusBarIconsUseBackground": false,
    "errorLens.margin": "3ch",
    "errorLens.enableOnDiffView": true,
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "hint",
        "info"
